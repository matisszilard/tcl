{
	"name": "tcl",
	"displayName": "TCL",
	"description": "TCL Language Support",
	"version": "0.0.2",
	"publisher": "rashwell",
	"engines": {
		"vscode": "^0.10.1"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/rashwell/tcl.git"
	},
	"categories": [
		"Languages"
	],
	"galleryBanner": {
		"color": "#9AB3CF",
		"theme": "light"
	},
	"contributes": {
		"languages": [
			{
				"id": "tcl",
				"aliases": [
					"Tcl",
					"tcl"
				],
				"extensions": [
					".tcl",
					".test"
				],
				"configuration": "./tcl.configuration.json"
			}
		],
		"grammars": [
			{
				"language": "tcl",
				"scopeName": "source.tcl",
				"path": "./syntaxes/tcl.tmLanguage"
			}
		],
        "snippets": [
            {
                "language": "tcl",
                "path": "./snippets/tcl.json"
            }
        ]
	}
}
